#hello world
print("hello world")

#hello world /t hello world
print("Hello World \t Hello World")

#hello world with new line
print("Hello World \n Hello World")

#Integer Input
a=int(input())
print(a)

#Float Input
a=float(input())
print("%.2f"%a)

#Character Input
a=input()
print(a)

#String Input
a=input()
print(a)

#Ascii-I
a=input("enter a character:")
print(ord(a))


#Ascii-II
a=int(input())
print(chr(a))
